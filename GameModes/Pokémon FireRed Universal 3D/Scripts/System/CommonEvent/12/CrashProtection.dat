version=2
:if:<Pokemon.count>=0
	@Player.stopmovement
	@Pokemon.add(999,100)
	@Battle.wild(999,1)
	@Player.turn(2)
	@Player.move(1)
	@Player.save
	@Register.change(System.ErrorLog.ErrorID4,2)
	@Script.start(System\GlobalErrorLog)
	@Player.warp(System\GlobalValidation.dat,0,0,0,0)
:endif
:if:<not><Pokemon.count>=0
	@Entity.remove(999999999)
:endif
:end
