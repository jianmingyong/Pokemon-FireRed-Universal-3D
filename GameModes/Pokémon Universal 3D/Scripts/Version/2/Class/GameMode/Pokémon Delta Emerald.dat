version=2
##############################
# Sub GameMode.PokemonDeltaEmerald
##############################

@Storage.set(str,StackTrace.Set,Version\2\Class\GameMode\PokemonDeltaEmerald.dat)
@Script.start(Version\2\Class\StackTrace\Set)

:if:<Storage.get(bool,Security.FileValidation.Checks)>=true
	@Storage.set(bool,Security.FileValidation.ScriptExist,true)
	:end
:endif

# GameMode Handler:
@Storage.set(str,GameMode.Name,Pokémon Delta Emerald)
@Storage.set(str,GameMode.Description,Pokémon Emerald Remake)
@Storage.set(str,GameMode.Version,0.54)
@Storage.set(str,GameMode.Author,jianmingyong)
@Storage.set(str,GameMode.MapPath,Pokémon Delta Emerald)
@Storage.set(str,GameMode.ScriptPath,Pokémon Delta Emerald)
@Storage.set(str,GameMode.StartMap,Nothing)
@Storage.set(str,GameMode.StartPosition,0,0,0,0)
@Storage.set(str,GameMode.StartRotation,0)
@Storage.set(str,GameMode.IntroMusic,P_OR_AS Opening Selection)

# Gamerules:
@Storage.set(int,GameMode.MaxBadges,0)
@Storage.set(int,GameMode.Badge.ID.1,0)

@Register.change(Global.GameName,<Storage.get(str,GameMode.Name)>)

# Global Register:
:if:<Register.registered(Pokémon Delta Emerald.Player.Gender)>=false
	@Register.register(Pokémon Delta Emerald.Player.Gender,str,Nothing)
:endif

# Local Register:
:if:<Register.registered(Pokémon Delta Emerald\LittleRootTown\LittleRootTown.Truck.Boy)>=false
	@Register.register(Pokémon Delta Emerald\LittleRootTown\LittleRootTown.Truck.Boy,int,0)
:endif

:if:<Register.registered(Pokémon Delta Emerald\LittleRootTown\LittleRootTown.Truck.Girl)>=false
	@Register.register(Pokémon Delta Emerald\LittleRootTown\LittleRootTown.Truck.Girl,int,0)
:endif
:end