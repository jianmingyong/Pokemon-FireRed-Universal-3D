##############################
# Function Security.FileExists
##############################
# Usage Code:
# @Storage.set(str,Security.FileExists,)
# @Script.start(Class\Security.FileExists)
#
# Return: Boolean

@Storage.set(str,StackTrace.Set,Class\Security\FileExists.dat)
@Script.start(Class\StackTrace\Set)

# Set a flag to call for script checks.
@Storage.set(bool,Security.FileValidation.Checks,true)

# Reset Call Flag.
@Storage.set(bool,Security.FileValidation.ScriptExist,false)
@Script.start(<Storage.get(str,Security.FileExists)>)
@Storage.set(bool,Global.Return,<Storage.get(bool,Security.FileValidation.ScriptExists)>)
	
@Storage.set(bool,Security.FileValidation.Checks,false)
