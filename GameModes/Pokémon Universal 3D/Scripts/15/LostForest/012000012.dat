version=2
@Sound.play(Select)
@Text.show(Are you sure you want to exit?)
@Options.setcancelindex(1)
@Options.show(Yes,No)
:when:Yes
	@Player.warp(System\GlobalIntro.dat,0,0,0,0)
	@Level.update
	@Script.start(System\Intro\GlobalBackground)
	@Script.start(System\GlobalIntro)
	:end
:when:No
	:end
:endwhen