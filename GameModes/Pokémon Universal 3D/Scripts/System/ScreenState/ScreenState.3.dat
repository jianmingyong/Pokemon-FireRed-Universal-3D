version=2
:while:<Storage.get(int,System.ScreenState)>=3
	:if:<not><Level.filename>=GlobalMain
		@Screen.fadeout
		@Player.turnto(0)
		@Player.warp(System\GlobalMain.dat,0,0,0,0)
		@Level.update
	:endif
	@Music.play(Mega Man Battle Network 6 - Theme Of Mega Man Battle Network 6)
	@camera.deactivethirdperson
	@Camera.fix
	@Script.start(System\ScreenState\Entity.Visible)
	@Screen.fadein
	@Text.show(Choose an option to change.)
	@Options.setcancelindex(2)
	@Options.show(New Battle System [<Register.value(System.New BattleV2 Exp)>],Sandbox Mode [<Register.value(System.SandBoxMode)>],Back)
	:when:New Battle System [true]
		@Register.change(System.New BattleV2 Exp,false)
	:when:New Battle System [false]
		@Register.change(System.New BattleV2 Exp,true)
	:when:Sandbox Mode [true]
		@Register.change(System.SandBoxMode,false)
	:when:Sandbox Mode [false]
		@Register.change(System.SandBoxMode,true)
	:when:Back
		@Player.save
		@Storage.set(int,System.ScreenState,0)
		@Script.start(System\ScreenState\ScreenState.0)
	:endwhen
:endwhile
:end