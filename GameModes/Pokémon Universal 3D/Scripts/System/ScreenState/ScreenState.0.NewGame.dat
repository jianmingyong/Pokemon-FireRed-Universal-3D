version=2
:while:<Storage.get(int,System.ScreenState)>=0
	@Text.show(Please Choose an option.)
	@Options.setcancelindex(2)
	@Options.show(Pokémon Emerald [12],Pokémon Dark Energy [16],Back)
	:when:Pokémon Emerald [12]
		:if:<Register.registered(System.Load.12)>=false
			@Storage.set(int,System.ScreenState,1)
			@Register.change(System.Game ID,12)
			@Script.start(System\ScreenState\ScreenState.1)
		:else
			@Text.show(Unable to restart.)
		:endif
	:when:Pokémon Dark Energy [16]
		:if:<Register.registered(System.Load.16)>=false
			#@Storage.set(int,System.ScreenState,1)
			#@Register.change(System.Game ID,16)
			#@Script.start(System\ScreenState\ScreenState.1)
			@Text.show(This is under development.~It is disabled for safty reasons.)
		:else
			@Text.show(Unable to restart.)
		:endif
	:when:Back
		:exitwhile
	:endwhen
:endwhile
@Script.start(System\ScreenState\ScreenState.0)
:end