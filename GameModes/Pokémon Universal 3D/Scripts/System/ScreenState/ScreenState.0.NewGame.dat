version=2
@Storage.set(int,System.ProcessValidGameMode,1)
@Storage.set(int,System.TotalValidGameMode,15)
@Storage.set(int,System.IndexValidGameMode,1)
:while:<not><Storage.get(int,System.ProcessValidGameMode)>><Storage.get(int,System.TotalValidGameMode)>
	:if:<Register.registered(System.Load.<Storage.get(int,System.ProcessValidGameMode)>)>=false
		:if:<Storage.get(int,System.ProcessValidGameMode)>=8
			@Storage.set(str,System.ValidGameMode.index.<Storage.get(int,System.IndexValidGameMode)>,Pokémon FireRed [8])
			@Storage.set(int,System.IndexValidGameMode,<Storage.get(int,System.IndexValidGameMode)>+1)
		:endif
		:if:<Storage.get(int,System.ProcessValidGameMode)>=12
			@Storage.set(str,System.ValidGameMode.index.<Storage.get(int,System.IndexValidGameMode)>,Pokémon Emerald [12])
			@Storage.set(int,System.IndexValidGameMode,<Storage.get(int,System.IndexValidGameMode)>+1)
		:endif
		:if:<Storage.get(int,System.ProcessValidGameMode)>=15
			@Storage.set(str,System.ValidGameMode.index.<Storage.get(int,System.IndexValidGameMode)>,Mystery Dungeon [15])
			@Storage.set(int,System.IndexValidGameMode,<Storage.get(int,System.IndexValidGameMode)>+1)
		:endif
	:endif
	@Storage.set(int,System.ProcessValidGameMode,<storage.get(int,System.ProcessValidGameMode)>+1)
:endwhile
@Storage.set(int,System.IndexValidGameMode,<Storage.get(int,System.IndexValidGameMode)>-1)
:while:<Storage.get(int,System.ScreenState)>=0
	@Text.show(Please Choose an option.)
	:if:<Storage.get(int,System.IndexValidGameMode)>=1
		@Options.setcancelindex(1)
		@Options.show(<Storage.get(str,System.ValidGameMode.index.1)>,Back)
		:when:Pokémon FireRed [8]
			@Text.show(Game not available yet.~Sorry for the inconvenience caused.)
		:when:Pokémon Emerald [12]
			@Storage.set(int,System.ScreenState,1)
			@Register.change(System.Game ID,12)
			@Script.start(System\ScreenState\ScreenState.1)
		:when:Mystery Dungeon [15]
			@Text.show(Game not available yet.~Sorry for the inconvenience caused.)
		:when:Back
			:exitwhile
		:endwhen
	:endif
	:if:<Storage.get(int,System.IndexValidGameMode)>=2
		@Options.setcancelindex(2)
		@Options.show(<Storage.get(str,System.ValidGameMode.index.1)>,<Storage.get(str,System.ValidGameMode.index.2)>,Back)
		:when:Pokémon FireRed [8]
			@Text.show(Game not available yet.~Sorry for the inconvenience caused.)
		:when:Pokémon Emerald [12]
			@Storage.set(int,System.ScreenState,1)
			@Register.change(System.Game ID,12)
			@Script.start(System\ScreenState\ScreenState.1)
		:when:Mystery Dungeon [15]
			@Text.show(Game not available yet.~Sorry for the inconvenience caused.)
		:when:Back
			:exitwhile
		:endwhen
	:endif
	:if:<Storage.get(int,System.IndexValidGameMode)>=3
		@Options.setcancelindex(3)
		@Options.show(<Storage.get(str,System.ValidGameMode.index.1)>,<Storage.get(str,System.ValidGameMode.index.2)>,<Storage.get(str,System.ValidGameMode.index.3)>,Back)
		:when:Pokémon FireRed [8]
			@Text.show(Game not available yet.~Sorry for the inconvenience caused.)
		:when:Pokémon Emerald [12]
			@Storage.set(int,System.ScreenState,1)
			@Register.change(System.Game ID,12)
			@Script.start(System\ScreenState\ScreenState.1)
		:when:Mystery Dungeon [15]
			@Text.show(Game not available yet.~Sorry for the inconvenience caused.)
		:when:Back
			:exitwhile
		:endwhen
	:endif
:endwhile
@Script.start(System\ScreenState\ScreenState.0)
:end