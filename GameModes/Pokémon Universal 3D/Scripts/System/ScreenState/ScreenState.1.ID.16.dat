	@Script.start(System\Intro\16\Intro.1)
	@Script.start(System\Intro\16\Intro.2)
	@Screen.fadein
	@Music.play(RouteMusic7)
	@Script.start(System\Intro\16\Intro.3)
	@Text.show(Hello there!)
	@Sound.play(Select)
