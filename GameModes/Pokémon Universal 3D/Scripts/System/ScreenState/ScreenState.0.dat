version=2
:while:<Storage.get(int,System.ScreenState)>=0
	:if:<not><Level.filename>=GlobalMain
		@Screen.fadeout
		@Camera.set(0,0,0,0,0)
		@Player.warp(System\GlobalMain.dat,0,0,0,0)
		@Level.update
	:endif
	@Music.play(title)
	@camera.deactivethirdperson
	@Camera.fix
	@Script.start(System\ScreenState\Entity.Visible)
	@Screen.fadein
	@Text.show(Please Choose an option.)
	@Options.show(New Game,Load Game,Options,Downloadable Content)
	:when:New Game
		@Script.start(System\ScreenState\ScreenState.0.NewGame)
	:when:Load Game
		:if:<Register.registered(System.Load.12)>=true
			@Text.show(Please Choose an option.)
			@Options.setcancelindex(1)
			@Options.show(Pokémon Emerald [12],Back)
			:when:Pokémon Emerald [12]
				@Script.start(System\GlobalAntiCheat)
				@Storage.set(int,System.ScreenState,2)
				@Player.warp(<Register.value(System.LastMap.12)>,<Register.value(System.PlayerPosition.X.12)>,<Register.value(System.PlayerPosition.Y.12)>+0.1,<Register.value(System.PlayerPosition.Z.12)>,<Register.value(System.PlayerFacing.12)>)
				@Level.update
			:when:Back
			:endwhen
		:else
			@Text.show(No valid save found.)
		:endif
	:when:Options
		@Script.start(System\ScreenState\ScreenState.0.Options)
	:when:Downloadable Content
		@Text.show(This is in beta feature right now.~It will not function.)
	:endwhen
:endwhile
:end