version=2
:if:<Register.value(System.SandBoxMode)>=false
	:if:<Storage.get(bool,System.EncounterStep.fix)>=false
		@Storage.set(int,System.EncounterStep.num1,12)
		@Storage.set(int,System.EncounterStep.num2,5)
		@Storage.set(int,System.EncounterStep.WalkedStep,1)
		@Storage.set(int,System.EncounterStep.num3,<Math.clamp(<Storage.get(int,System.EncounterStep.num1)>-<Storage.get(int,System.EncounterStep.WalkedStep)>,5,12)>)
		@Storage.set(bool,System.EncounterStep.fix,true)
	:endif
	@Storage.set(int,System.EncounterStep.WalkedStep,<Storage.get(int,System.EncounterStep.WalkedStep)>+1)
	@Storage.set(int,System.EncounterStep.num3,<Math.clamp(<Storage.get(int,System.EncounterStep.num1)>-<Storage.get(int,System.EncounterStep.WalkedStep)>,5,12)>)
:endif
:end