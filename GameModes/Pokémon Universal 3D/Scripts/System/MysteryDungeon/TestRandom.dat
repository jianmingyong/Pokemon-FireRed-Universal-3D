version=2
# This is a API for Random Dugeon Generator.

# Registers Used:
# Prefix: System\Intro\15\MysteryDungeon.

# MinRoomSizeX
# MaxRoomSizeX
# MinRoomSizeZ
# MaxRoomSizeZ

# Define Room Size.
@Register.change(System\Intro\15\MysteryDungeon.MinRoomSizeX,5)
@Register.change(System\Intro\15\MysteryDungeon.MaxRoomSizeX,10)
@Register.change(System\Intro\15\MysteryDungeon.MinRoomSizeZ,5)
@Register.change(System\Intro\15\MysteryDungeon.MaxRoomSizeZ,10)
# Dig a Random Location.
@Title.add(Generating... Please wait for a while.,2147483647,255,255,255,2,true,0,0)
@Script.start(System\MysteryDungeon\RoomGenerator)
@Script.start(System\MysteryDungeon\RoomGenerator)
@Script.start(System\MysteryDungeon\RoomGenerator)
@Script.start(System\MysteryDungeon\RoomGenerator)
@Script.start(System\MysteryDungeon\RoomGenerator)
# Warp Player to a walkable zone.
@Player.warp(<System.random(<Register.value(System\Intro\15\MysteryDungeon.CurrentRoomLocationX)>,<Register.value(System\Intro\15\MysteryDungeon.CurrentRoomLocationX)>+<Register.value(System\Intro\15\MysteryDungeon.FinalRoomSizeX)>-1)>,0.1,<System.random(<Register.value(System\Intro\15\MysteryDungeon.CurrentRoomLocationZ)>,<Register.value(System\Intro\15\MysteryDungeon.CurrentRoomLocationZ)>+<Register.value(System\Intro\15\MysteryDungeon.FinalRoomSizeZ)>-1)>)
@Level.update
@Title.clear
:end
