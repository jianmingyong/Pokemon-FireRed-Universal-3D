version=2
:while:<Entity.opacity(124)><100
	@Entity.setopacity(124,<Entity.opacity(124)>+10)
	@Level.wait(1)
	@Level.update
	:if:<Entity.opacity(124)>=100
		:end
	:endif
:endwhile
:while:<Entity.opacity(124)>>0
	@Entity.setopacity(124,<Entity.opacity(124)>-10)
	@Level.wait(1)
	@Level.update
	:if:<Entity.opacity(124)>=0
		:end
	:endif
:endwhile