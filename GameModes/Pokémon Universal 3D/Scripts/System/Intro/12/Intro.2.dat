version=2
:while:<Entity.opacity(122)><100
	@Entity.setopacity(122,<Entity.opacity(122)>+10)
	@Level.wait(1)
	@Level.update
	:if:<Entity.opacity(122)>=100
		:end
	:endif
:endwhile
:while:<Entity.opacity(122)>>0
	@Entity.setopacity(122,<Entity.opacity(122)>-10)
	@Level.wait(1)
	@Level.update
	:if:<Entity.opacity(122)>=0
		:end
	:endif
:endwhile