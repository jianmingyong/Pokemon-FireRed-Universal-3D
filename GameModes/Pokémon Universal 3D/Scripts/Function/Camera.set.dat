version=2
# Set Camera to a Location.
# Usage:
# @Storage.set(int,Camera.x,0)
# @Storage.set(int,Camera.y,0)
# @Storage.set(int,Camera.z,0)
# @Script.start(Function\Camera.set)
@camera.setx(<Storage.get(int,Camera.x)>)
@camera.sety(<Storage.get(int,Camera.y)>)
@camera.setz(<Storage.get(int,Camera.z)>)
:end